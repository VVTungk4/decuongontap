package baitapdecuong5;

public class SinhVienUTT extends SinhVien{
	private String donVi;
	private double luong;
	public SinhVienUTT() {
	}
	public SinhVienUTT(String donVi, double luong) {
		super();
		this.donVi = donVi;
		this.luong = luong;
	}
	public void hienThi() {
		super.hienThi();
		System.out.println(""+donVi+"\t"+luong+"\t");
	}
	
	
}
